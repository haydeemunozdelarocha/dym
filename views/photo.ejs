<html>
<head>
  <meta charset="UTF-8">
  <title>Acarreo - Captura</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
</head>
<body>
<div id="content-container">
<form action="/camera" method="GET" enctype="application/x-www-form-urlencoded">
   <div class="form-group" id="camion_id">
      <label for="camion_id">Cami√≥n ID</label>
      <input type="text" class="form-control" id="scanner" name="camion_id" oninput="getCamion()" required autofocus autocomplete="off">
      <div id="search-status"></div>
    </div>
        <div class="form-group" id="materialinfo">
    <div id="material">
      <label for="material">Proveedor Material:</label>
          <select class="form-control" name="proveedor_id" id="proveedor_id" onchange="getMateriales()" required disabled>
            <option value="">Proveedor</option>
    </select>
    </div>
    </div>
    <div class="form-group" id="materialinfo">
    <div id="material">
      <label for="material">Material</label>
        <select class="form-control" name="material_id" id="material_id" onchange="getMaterial()" required disabled>
        <option value="">Material</option>
        </select>
    </div>
    </div>
    <div class="form-group" id="precio">
      <label for="precio">Precio Unitario</label>
      <input type="text" class="form-control" id="precio-input" name="precio" readonly="readonly">
    </div>
    <div class="form-group" name="zona" id="zona">
      <label for="zona">Zona</label>
        <select class="form-control" id="zona" name="zona" required >
            <option value="">Zonas</option>
            <option >Zona 1</option>
            <option >Zona 2</option>
            <option >Zona 3</option>
        </select>
    </div>
    <div class="form-group" name="foto" id="foto">
      <button type="submit" class="btn btn-primary">Capturar Imagen</button>
    </div>
  <button type="submit" class="btn btn-primary">Ver ticket</button>
</form>
</div>
<script src="/javascripts/script.js" type="text/javascript" charset="utf-8" async defer></script>
</body>
</html>
