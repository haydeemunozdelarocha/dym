<html>
<head>
    <meta charset="UTF-8">
    <title>Empleados</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
  <% include ./partials/residentesheader.ejs %>
  <div id="content-container">
  <ul class="nav">
  <li><a href="empleados/nuevo">Agregar</a></li>
</ul>
  <div id="table">
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Empleados</div>

  <!-- Table -->
    <table class="table">
    <% for(var i=0; i<empleados.length; i++) {%>
    <tr>
      <td><%= empleados[i].id %></td>
     <td><%= empleados[i].nombre %></td>
      <td><%= empleados[i].obra %></td>
      <td><a href="empleados/<%= empleados[i].id %>"><span class="glyphicon glyphicon-edit"></span></a></td>
      <td><a href="empleados/<%= empleados[i].id %>"><span class="glyphicon glyphicon-remove-circle"></span></a></td>
     </tr>
    <% } %>
    </table>
  </div>
</div>
</body>
</html>
